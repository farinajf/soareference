<?xml version="1.0" encoding="UTF-8"?>
<proxy name="EmpleadosProxySecure" startOnLoad="true" statistics="enable" transports="https http" xmlns="http://ws.apache.org/ns/synapse">
    <target>
        <endpoint key="EmpledadosWSDLEndpoint"/>
        <inSequence>
            <log level="full">
                <property name="MESSAGE" value="Sending request to EmpleadosProxySecure......."/>
            </log>
            <header action="remove" name="wsse:Security" scope="default" xmlns:wsse="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-secext-1.0.xsd"/>
            <log level="full">
                <property name="MESSAGE" value="Sending message to EmpledadosWSDLEndpoint........"/>
            </log>
        </inSequence>
        <outSequence>
            <send/>
        </outSequence>
        <faultSequence/>
    </target>
    <publishWSDL uri="http://wso2b:8280/services/Empleados1DSS?wsdl"/>
    <policy key="gov:amtega/ws-policy/BasicHTTPSecurityPolicy.xml"/>
    <enableSec/>
</proxy>
