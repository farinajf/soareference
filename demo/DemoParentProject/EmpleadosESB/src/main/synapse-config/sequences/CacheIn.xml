<?xml version="1.0" encoding="UTF-8"?>
<sequence name="CacheIn" trace="disable" xmlns="http://ws.apache.org/ns/synapse">
    <log>
        <property name="MESSAGE" value="Before Cache In...."/>
    </log>
    <cache collector="false" hashGenerator="org.wso2.carbon.mediator.cache.digest.DOMHASHGenerator" id="micache" maxMessageSize="2000" scope="per-host" timeout="10">
        <onCacheHit/>
        <implementation maxSize="1000" type="memory"/>
    </cache>
</sequence>
